{
  "name": "react-native-maps",
  "description": "React Native Mapview component for iOS + Android",
  "source": "src/index",
  "main": "lib/index.js",
  "author": "Leland Richardson <leland.m.richardson@gmail.com>",
  "homepage": "https://github.com/react-native-maps/react-native-maps#readme",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "tscheck": "tsc --noEmit",
    "format-check": "prettier --check .",
    "build": "tsc --project tsconfig.build.json",
    "test": "jest",
    "prepare": "husky install",
    "release": "semantic-release",
    "bundle-install": "cd example && bundle install",
    "pod-install": "cd example/ios && bundle exec pod install",
    "bootstrap": "yarn --cwd example && yarn && yarn bundle-install && yarn pod-install"
  },
  "files": [
    "lib",
    "android",
    "ios",
    "react-native-maps.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-native-maps/react-native-maps"
  },
  "keywords": [
    "react",
    "react-native",
    "react-component",
    "map",
    "mapview",
    "google-maps",
    "mapkit"
  ],
  "peerDependencies": {
    "react": ">= 19.0.0",
    "react-native": ">= 0.78.0",
    "react-native-web": ">= 0.11"
  },
  "peerDependenciesMeta": {
    "react-native-web": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/runtime": "^7.26.9",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@react-native-community/cli": "15.1.3",
    "@react-native/babel-preset": "0.78.0",
    "@react-native/eslint-config": "0.78.0",
    "@react-native/metro-config": "0.78.0",
    "@react-native/typescript-config": "0.78.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-native": "0.73.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "husky": "8.0.3",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react": "^19.0.0",
    "react-native": "^0.78.0",
    "react-test-renderer": "19.0.0",
    "semantic-release": "24.2.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.13"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "e2e"
    ]
  },
  "codegenConfig": {
    "name": "RNMapsSpecs",
    "type": "all",
    "jsSrcsDir": "./src/specs",
    "outputDir": {
      "ios": "ios/generated",
      "android": "android/generated"
    },
    "includesGeneratedCode": true,
    "android": {
      "javaPackageName": "com.rnmaps.fabric"
    }
  },
  "engines": {
    "node": ">=18"
  }
}
