{
  "name": "react-native-maps",
  "version": "0.0.0",
  "description": "React Native Mapview component for iOS + Android",
  "source": "./src/index.ts",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!*.test.js",
    "!**/.*"
  ],
  "scripts": {
    "lint": "yarn prettier:check && yarn lint:js:check && yarn lint:android:check && yarn lint:ios:check",
    "lint:js:check": "eslint  \"**/*.{js,ts,tsx}\" --max-warnings 0",
    "lint:android:check": "google-java-format --set-exit-if-changed --dry-run --glob=\"android/src/**/*.java\"",
    "lint:android:fix": "google-java-format --replace --glob=\"android/src/**/*.java\"",
    "lint:ios:check": "clang-format --dry-run --glob=\"ios/**/*.{h,cpp,m,mm}\" --style=Google -n -Werror",
    "lint:ios:fix": "clang-format -i --glob=\"ios/**/*.{h,cpp,m,mm}\" --style=Google",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "yarn prettier:fix && yarn lint:android:fix && yarn lint:ios:fix",
    "build": "tsc --project tsconfig.build.json",
    "test": "jest",
    "prepare": "husky install && bob build",
    "release": "semantic-release",
    "bundle-install": "cd example && bundle install",
    "pod-install": "cd example/ios && bundle exec pod install",
    "bootstrap": "yarn --cwd example && yarn && yarn bundle-install && yarn pod-install --repo-update",
    "clean": "del-cli android/build example/android/build example/android/app/build example/android/app/.cxx example/ios/build example/ios/Pods lib"
  },
  "keywords": [
    "react",
    "react-native",
    "react-component",
    "map",
    "mapview",
    "google-maps",
    "mapkit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-native-maps/react-native-maps.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-native-maps/react-native-maps/issues"
  },
  "homepage": "https://github.com/react-native-maps/react-native-maps#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.16"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@react-native-community/cli": "15.0.1",
    "@react-native/eslint-config": "^0.73.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "clang-format": "^1.8.0",
    "commitlint": "^17.0.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "google-java-format": "^2.0.0",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "19.0.0",
    "react-native": "0.78.0",
    "react-native-builder-bob": "^0.36.0",
    "semantic-release": "^24.2.3",
    "turbo": "^1.10.7",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Changelog"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "codegen",
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNMapsSpecs",
    "type": "all",
    "jsSrcsDir": "./src/specs",
    "outputDir": {
      "ios": "ios/generated",
      "android": "android/generated"
    },
    "android": {
      "javaPackageName": "com.rnmaps.fabric"
    },
    "includesGeneratedCode": true
  }
}
